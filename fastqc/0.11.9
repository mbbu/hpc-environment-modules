set appname fastqc
set version 0.11.9
set prefix  /opt/apps/${appname}/${version}
set url     "http://www.bioinformatics.babraham.ac.uk/projects/fastqc/"
set msg     "FastQC is a quality control tool for high throughput sequence data.\nOfficial site: $url\n"

conflict    $appname

module-whatis $msg

prepend-path    PATH    ${prefix}

